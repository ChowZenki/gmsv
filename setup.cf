#服务器调试等级
#有用的值为0、1、3
#0：不报告任何错误
#1: 只报告关键错误 其他错误将忽略
#3：报告所有错误
debuglevel=3
#GMSV启动占用内存总量公式为 (usememoryunit*usememoryunitnum)/(1024*1024) 兆
#每内存单元大小 单位：字节
usememoryunit=128
#有多少个内存单元
usememoryunitnum=2500000
# 帐号服务器 SAAC的服务器IP地址
acserv=127.0.0.1
# 帐号服务器 SAAC的服务器端口
acservport=9200
# 连接到帐号服务器 SAAC的密码
acpasswd=test
# 服务器名字
gameservname=一线
# 服务器的ID
gameservid=一线
# 是否允许族战
allowmanorpk=1
# 服务器端口
port=9065
# 服务器SN
servernumber=1
# 重复使用socket地址
# 当9065端口被占用的情况下会不断重试使用该端口
reuseaddr=1
#套接字延迟 TCP_NODELAY 不建议开放
nodelay=0
#以下请设置为0 不写日志
log_write_time=0
log_io_time=0
log_game_time=0
# 记录LOG
log_netloop_faster=1
# 每次读取SAAC的数目
saacwritenum=10000
# 每次写入SAAC的数目
saacreadnum=10000
# 最大在线人数
fdnum=10
# Max pets numbers
# 最大在线宠数
petnum=100
# 其他最大数目 包含NPC数目+玩家数目
othercharnum=10000
# 最大对象数目
objnum=15000
# 最大道具数目
itemnum=1000
# 最大战斗数目
battlenum=100
#顶层文件夹
topdir=. 
#地图文件夹
mapdir=data/map
#地图设置文件
maptilefile=data/map/mapset.txt
#战斗地图文件
battlemapfile=data/map/battlemap.txt
#道具文件
itemset6file=data/itemset6.txt
#无敌NPC或不可战斗NPC
invinciblefile=data/inv.txt
#显示位置文件
appearpositionfile=data/appear.txt
#所有名称文件
titlenamefile=data/titlename.txt
#所有设置文件
titleconfigfile=./data/titleconfig.txt
#遇到怪的文件
