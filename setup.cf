#宠物基础设置文件
enemybasefile=./data/enemybase1.txt
#遇敌组文件
groupfile=./data/group1.txt
# ?(精灵和魔法文件?)
magicfile=./data/magic.txt
#攻击魔法文件
attmagicfile=./data/attmagic.bin
#宠物技能文件
petskillfile2=./data/petskill2.txt
petskillfile1=./data/petskill.txt
# ?(物品组成文件?还是素材文件?)
itematomfile=./data/itematom.txt
# ?(遇敌?)
effectfile=data/effect.txt
# 猜迷的文件
quizfile=data/question.txt
#每次发送封包和接收封包时候记录封包内容的文件
#设置为空则不写该文件
lsgenlogfilename=
#GM帐号文件
gmsetfile=./data/gmset.txt
# 还原玩家资料目录 可防止当机回档
storedir=./data/store
# NPC 目录
npcdir=data/npc
# 日志目录
logdir=./log
# 日志配置文件
logconfname=log.cf
#GM密码
chatmagicpasswd=jzgm1
# ?(是否所有人都能使用GM权限)
chatmagiccdkeycheck=0
# NPC下最大使用文件总数
filesearchnum=9000
# 注册NPC_fuction类型文件最大数
npctemplatenum=256
# NPC最大数
npccreatenum=9000
# 走路间隔
walkinterval=250
# CleanAll间隔
CAinterval=250
# CleanDest间隔
CDinterval=250
# Character data save interval(数据存储间隔?)
CharSaveinterval=300
# 执行一次 netloop_faster 所消耗时间
Onelooptime=6
#(宠物消失时间)
Petdeletetime=60
#(物品消失时间)
