# 缓冲区大小
acwbsize=500000
# 如果发现玩家使用非法手段 比如说改封包跳NPC判断 或者点数异常 则关进监狱
erruser_down=1
#人物职业技能
profession=./data/profession.txt
#退出团队的道具
